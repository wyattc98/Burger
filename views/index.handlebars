<div class="header">
    <img
        src="data:image/png;base64,"
    >

    <h1>Eat-da-Burger!</h1>
</div>

<ul>
    {{#each burgers}}
    {{#unless devoured}}
    {{burgers/burger-block}}
    {{/unless}}
    {{/each}}
</ul>

<ul>
    {{#each burgers}}
    {{#if devoured}}
    {{> burgers/burger-block devoured=true}}
    {{/if}}
    {{/each}}
</ul>

<form class="create-form">
    <div class="form-group">
        <label for="bn">Enter Burger Here!</label>
        <input type="name" class="form-control" id="bn" placeholder="Burger Name"> </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>